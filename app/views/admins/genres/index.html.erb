<%= form_for(@genre, url:admins_genres_path) do |f| %>

<%= f.label "ジャンル名" %>
<%= f.text_field :name %>

<%= f.submit %>

<% end %>

<% @genres.each do |genre| %>

<%= genre.name %>

<%= link_to edit_admins_genre_path(genre) do %>
<label class="btn btn-primary btn-md">編集する</label>
<% end %>

<% end %>